{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["esnext", "ES2015"],
    "jsx": "react"
  },
  "baseUrl": "src"
}
